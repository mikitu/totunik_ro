{
  "kind": "collectionType",
  "collectionName": "business_partner_pages",
  "info": {
    "singularName": "business-partners-page",
    "pluralName": "business-partner-pages",
    "displayName": "Business Partners Page",
    "description": "Individual business partner sector pages (Residential, Industrial, Medical, Retail)"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "sector": {
      "type": "enumeration",
      "enum": [
        "residential",
        "industrial",
        "medical",
        "retail"
      ],
      "required": true
    },
    "hero": {
      "type": "component",
      "repeatable": false,
      "component": "business-partners.hero-section",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "introduction": {
      "type": "component",
      "repeatable": false,
      "component": "business-partners.introduction-section",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "caseStudies": {
      "type": "component",
      "repeatable": false,
      "component": "business-partners.case-studies-section",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "testimonials": {
      "type": "component",
      "repeatable": false,
      "component": "business-partners.testimonials-section",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "cta": {
      "type": "component",
      "repeatable": false,
      "component": "business-partners.cta-section",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
