{
  "name": "totunik",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:web": "pnpm --filter web dev",
    "dev:cms": "pnpm --filter cms develop",
    "dev": "concurrently \"pnpm dev:web\" \"pnpm dev:cms\"",
    "build": "pnpm --recursive build"
  },
  "devDependencies": {
    "concurrently": "^8.0.0"
  }
}